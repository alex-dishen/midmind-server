generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id         String    @id @default(uuid())
  first_name String
  last_name  String
  email      String    @unique
  password   String
  created_at DateTime  @default(now())
  updated_at DateTime?
  is_deleted Boolean   @default(false)
  deleted_at DateTime?
  deleted_by String?

  @@map("users")
}
